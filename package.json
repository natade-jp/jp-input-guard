	"type": "module",
	"source": "./src/jp-input-guard.js",
	"main": "./dist/cjs/jp-input-guard.cjs",
	"module": "./dist/esm/jp-input-guard.js",
	"types": "./dist/types/jp-input-guard.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/jp-input-guard.d.ts",
			"import": "./dist/esm/jp-input-guard.js",
			"require": "./dist/cjs/jp-input-guard.cjs",
			"browser": "./dist/umd/jp-input-guard.js",
			"default": "./dist/esm/jp-input-guard.js"
		}
	},
	"unpkg": "./dist/umd/jp-input-guard.min.js",
	"jsdelivr": "./dist/umd/jp-input-guard.min.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/natade-jp/jp-input-guard.git"
	},
	"bugs": {
		"url": "https://github.com/natade-jp/jp-input-guard/issues"
	},
	"homepage": "https://github.com/natade-jp/jp-input-guard",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "node ./scripts/package.build.js",
		"test": "jest",
		"test_target": "node ./scripts/package.test.js"
	},
	"directories": {
		"src": "./src",
		"build": "./build"
	},
	"jest": {
		"verbose": true,
		"rootDir": "./src",
		"moduleFileExtensions": [
			"js",
			"mjs"
		],
		"testMatch": [
			"**/__tests__/**/*.?(m)js?(x)",
			"**/?(*.)(spec|test).?(m)js?(x)"
		],
		"transform": {
			"^.+\\.(js|mjs)$": "babel-jest"
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@rollup/plugin-buble": "^1.0.3",
		"@rollup/plugin-commonjs": "^29.0.0",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^25.0.10",
		"babel-jest": "^30.2.0",
		"baseline-browser-mapping": "^2.9.17",
		"eslint": "^9.39.2",
		"eslint-plugin-jest": "^29.0.1",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"globals": "^17.1.0",
		"jest": "^30.2.0",
		"jsonc-eslint-parser": "^2.4.2",
		"ntfile": "^2.0.0",
		"rollup": "^4.56.0",
		"rollup-plugin-dts": "^6.3.0",
		"typescript": "^5.9.3"
	}
}
